{
  "name": "udacity_storefront_backend",
  "version": "0.1.0",
  "description": "Udacity Fullstack Javascript Developer Nanodegree project two. RESTful API for a store with node, express and postgress. ",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "tsc": "tsc"
  },
  "author": "Tabi Idris",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1",
    "supertest": "^6.3.3",
    "jasmine": "^3.6.4",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/pg": "^7.14.7",
    "prettier": "^2.8.1",
    "jasmine-spec-reporter": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
